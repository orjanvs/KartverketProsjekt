@model List<KartverketProsjekt.Models.MapReportModel>

@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<div class="container py-5">
    @if (Model != null && Model.Any())
    {
        <table class="table">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Dato</th>
                    <th>Beskrivelse</th>
                    <th>GeoJson</th>
                    <th>Status</th>
                    
                </tr>
            </thead>
            <tbody>
                @foreach (var mapReport in Model)
                {
                    <tr>
                        <td>@mapReport.MapReportId</td>
                        <td>@mapReport.SubmissionDate</td>
                        <td>@mapReport.Description</td>
                        <td>@mapReport.GeoJson</td>
                        <td>@mapReport.CaseStatus</td>
                    </tr>

                }
            </tbody>
        </table>
    }
    else
    {
        <p>Ingen innmeldinger funnet!</p>
    }
</div>